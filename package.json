{
  "name": "from-to-when",
  "version": "0.0.3",
  "description": "From? To? When?",
  "main": "./build/form",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "scripts": {
    "dist": "NODE_ENV=production browserify index.js -s FromToWhen -t babelify -t [ browserify-css --minify ] | uglifyjs -cm > dist/ftw.js",
    "lint": "standard",
    "prepublish": "babel lib --out-dir build",
    "start": "budo example.js --live --open -- -t babelify -t browserify-css -t envify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/ftw"
  },
  "keywords": [
    "geocoder",
    "route"
  ],
  "author": "Conveyal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conveyal/ftw/issues"
  },
  "homepage": "https://github.com/conveyal/ftw",
  "dependencies": {
    "react-select-geocoder": "0.0.1"
  },
  "peerDependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-css": "^0.8.4",
    "budo": "^8.0.0",
    "envify": "^3.4.0",
    "normalize.css": "^3.0.3",
    "qs": "^6.0.2",
    "standard": "^5.4.1",
    "uglify-js": "^2.6.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
