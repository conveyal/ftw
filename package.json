{
  "name": "from-to-when",
  "version": "0.0.4",
  "description": "From? To? When?",
  "main": "./build/form",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "scripts": {
    "dist": "NODE_ENV=production browserify index.js -s FromToWhen -t babelify -t [ browserify-css --minify ] | uglifyjs -cm > dist/ftw.min.js",
    "lint": "standard",
    "prepublish": "babel lib --out-dir build",
    "start": "budo example.js --live -- -t babelify -t browserify-css -t envify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/ftw"
  },
  "keywords": [
    "geocoder",
    "route"
  ],
  "author": "Conveyal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conveyal/ftw/issues"
  },
  "homepage": "https://github.com/conveyal/ftw",
  "dependencies": {
    "react-pure-render": "^1.0.2",
    "react-select-geocoder": "0.2.1"
  },
  "peerDependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-css": "^0.9.2",
    "budo": "^9.2.0",
    "envify": "^3.4.0",
    "normalize.css": "^5.0.0",
    "standard": "^8.2.0",
    "uglify-js": "^2.6.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
